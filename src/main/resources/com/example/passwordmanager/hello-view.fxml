<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER_LEFT" fillWidth="false" prefHeight="130.0" prefWidth="250.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.passwordmanager.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Pane prefHeight="26.0" prefWidth="171.0">
        <Label layoutY="-6.0" text="Select AccountType:"/>
        <ChoiceBox fx:id="acType" layoutX="117.0" layoutY="-10.0" prefHeight="25.0" prefWidth="98.0">

        </ChoiceBox>
    </Pane>
    <Pane prefHeight="144.0" prefWidth="171.0">
        <Label layoutY="8.0" text="Type Key:"/>
        <PasswordField id="typeKey" layoutX="61.0" layoutY="5.0" prefHeight="25.0" prefWidth="140.0"/>
    </Pane>
    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#CpPassword" text="Copy password" />
</VBox>
